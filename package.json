{
  "name": "meteor-cypress-debug",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "start-e2e": "meteor test --full-app --port 3000 --driver-package tmeasday:acceptance-test-driver",
    "test": "meteor test --once --driver-package meteortesting:mocha",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha",
    "visualize": "meteor --production --extra-packages bundle-visualizer",
    "cypress": "start-server-and-test start-e2e http://localhost:3000 cypress-open",
    "cypress-open": "./node_modules/cypress/bin/cypress open"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.2",
    "chai": "^4.2.0",
    "cypress": "^3.6.1",
    "meteor-node-stubs": "^0.4.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "start-server-and-test": "^1.10.6"
  }
}